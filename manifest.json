{
  "manifest_version": 3,
  "name": "Quick QR Code Generator",
  "version": "1.0",
  "description": "Generate a QR code for the current webpage.",
  "icons": {
    "48": "icon.webp"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.webp"
  }
}
